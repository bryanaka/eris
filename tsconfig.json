{
  "compilerOptions": {
    "target": "ES2020",
    "module": "es2020",
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "outDir": "dist",
    "removeComments": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./",
    "paths": {
      "eris/config/*": ["config/*"],
      "eris/*": ["src/*"],
      "eris": ["src/eris"],
      "discord": ["node_modules/discord.js/src/index.js"],
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
